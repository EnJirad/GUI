print("XHUB GUI")
Screen = Instance.new("ScreenGui")
Screen.Parent = game.Players.LocalPlayer.PlayerGui

Window = Instance.new("Frame", Screen)
Window.BorderSizePixel = 0
Window.BackgroundColor3 = Color3.fromRGB(85, 85, 127)
Window.Position = UDim2.new(0.5, -250, 0.5, -160) -- Center position
Window.Size = UDim2.new(0, 500, 0, 320)
Window.Active = true


Container = Instance.new("ScrollingFrame", Window)
Container.Size = UDim2.new(1,0,0.85,0)
Container.Position = UDim2.new(0,0,0.15,0)
Container.BackgroundColor3 = Color3.fromRGB(10,150,200)
Container.BorderSizePixel =0

Title  = Instance.new("TextLabel", Window)
Title.Size = UDim2.new(1,0,0.15,0)
Title.Position = UDim2.new(0,0,0,0)
Title.BackgroundTransparency = 1
Title.Text = "Lol i dont liek FE fak you. Made by goldengabe."

CloseBTN = Instance.new("TextButton", Window)
CloseBTN.Text = "X"
CloseBTN.Size = UDim2.new(0.1,0,0.1,0)
CloseBTN.BorderSizePixel = 0

CloseBTN.MouseButton1Down:connect(function()
	Window.Visible =false
end)

SuperSpeed = Instance.new("TextButton", Container)
SuperSpeed.Size = UDim2.new(0.45,0,0.1,0)
SuperSpeed.Position = UDim2.new(0.05,0,0,0)
SuperSpeed.BorderSizePixel = 0
SuperSpeed.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
SuperSpeed.Text = "SuperSpeed"

SuperSpeed.MouseButton1Down:connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 200
end)

SuperJump = Instance.new("TextButton", Container)
