ClickToTp = Instance.new("TextButton", Container)
ClickToTp.Size = UDim2.new(0.45,0,0.1,0)
ClickToTp.Position = UDim2.new(0.05,0,0.11,0)
ClickToTp.BorderSizePixel = 0
ClickToTp.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
ClickToTp.Text = "Click to tp"

ClickToTp.MouseButton1Down:connect(function()
	player1=game.Players.LocalPlayer

q=Instance.new('HopperBin', player1.Backpack)
q.Name = 'Click Teleport'


bin = q



function teleportPlayer(pos)
local player = game.Players.LocalPlayer
if player == nil or player.Character == nil then return end
player.Character.HumanoidRootPart.CFrame = CFrame.new(Vector3.new(pos.x, pos.y + 7, pos.z))
end


enabled = true
function onButton1Down(mouse)
if not enabled then
return
end

local player = game.Players.LocalPlayer
if player == nil then return end


enabled = false
local cf = mouse.Hit
local v = cf.lookVector

teleportPlayer(cf.p)

wait()
enabled = true

end

function onSelected(mouse)
mouse.Icon = "rbxasset://textures\\ArrowCursor.png"
mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

bin.Selected:connect(onSelected)
end)


MM2ShowMurderer = Instance.new("TextButton", Container)
MM2ShowMurderer.Size = UDim2.new(0.45,0,0.1,0)
MM2ShowMurderer.Position = UDim2.new(0.5,0,0.11,0)
MM2ShowMurderer.BorderSizePixel = 0
MM2ShowMurderer.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
MM2ShowMurderer.Text = "MM2: Show Murderer"

MM2ShowMurderer.MouseButton1Down:connect(function()
	for index, child in pairs(game.Players:GetChildren()) do
    	if child.Backpack:FindFirstChild("Knife") ~= nil then
	MM2ShowMurderer.Text = "OMG ITS "..child.Name
	wait(2)
	MM2ShowMurderer.Text = "MM2: Show Murderer"
end
	end
end)

NoClip = Instance.new("TextButton", Container)
NoClip.Size = UDim2.new(0.45,0,0.1,0)
NoClip.Position = UDim2.new(0.05,0,0.22,0)
NoClip.BorderSizePixel = 0
NoClip.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
NoClip.Text = "NoClip"

NoClip.MouseButton1Down:connect(function()
noclip = false
game:GetService('RunService').Stepped:connect(function()
if noclip then
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
plr = game.Players.LocalPlayer
mouse = plr:GetMouse()
mouse.KeyDown:connect(function(key)

if key == "e" then
noclip = not noclip
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
NoClip.Text = "Press 'e' to toggle No Clip"
wait(1)
NoClip.Text = "NoClip"
end)


XRAY = Instance.new("TextButton", Container)
XRAY.Size = UDim2.new(0.45,0,0.1,0)
XRAY.Position = UDim2.new(0.5,0,0.33,0)
XRAY.BorderSizePixel = 0
XRAY.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
XRAY.Text = ""

XRAY.MouseButton1Down:connect(function()
	for index, child in pairs(game.Workspace:GetChildren()) do
    		if child.ClassName == "Part" then
			child.Transparency = 0.5
		end
	end
end)

MM2ShowS = Instance.new("TextButton", Container)
MM2ShowS.Size = UDim2.new(0.45,0,0.1,0)
MM2ShowS.Position = UDim2.new(0.5,0,0.22,0)
MM2ShowS.BorderSizePixel = 0
MM2ShowS.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
MM2ShowS.Text = "MM2: Show Sheriff"

MM2ShowS.MouseButton1Down:connect(function()
	for index, child in pairs(game.Players:GetChildren()) do
        if child:FindFirstChild("Backpack") ~= nil then
    	if child.Backpack:FindFirstChild("Gun") ~= nil then
	MM2ShowS.Text = "OMG ITS "..child.Name
	wait(2)
	MM2ShowS.Text = "MM2: Show Sheriff"
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace[child.Name].HumanoidRootPart.Position)
end
	end
end
end)

MM2Show = Instance.new("TextButton", Container)
MM2Show.Size = UDim2.new(0.45,0,0.1,0)
MM2Show.Position = UDim2.new(0.5,0,0.33,0)
MM2Show.BorderSizePixel = 0
MM2Show.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
MM2Show.Text = "Teleport to next criminal"

MM2Show.MouseButton1Down:connect(function()
	for index, child in pairs(game.Players:GetChildren()) do
         if child.Team == game.Teams.Criminal then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(child.Character.HumanoidRootPart.Position)
         end

    end
end)
